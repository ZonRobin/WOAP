    "react-beautiful-dnd": "^7.1.0",
    "react-contexify": "3.0.0",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.2.0",
    "react-draggable": "3.0.5",
    "react-fontawesome": "^1.6.1",
