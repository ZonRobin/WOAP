    "babel-eslint": "8.2.3",
    "babel-jest": "22.4.3",
    "babel-loader": "7.1.4",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "6.26.0",
    "buffer": "^5.0.8",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.0",
