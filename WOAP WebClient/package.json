    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "fs-extra": "5.0.0",
    "html-to-react": "^1.3.1",
    "html-webpack-plugin": "3.2.0",
    "jest": "22.4.3",
    "nexusui": "2.0.5",
    "object-assign": "4.1.1",
    "osc-min": "1.1.1",
    "postcss-flexbugs-fixes": "3.3.0",
    "postcss-loader": "2.1.4",
    "promise": "8.0.1",
    "prop-types": "^15.6.0",
